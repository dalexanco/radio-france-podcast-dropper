{
  "name": "france-radio-podcast-dropper",
  "version": "0.1.0",
  "description": "A CLI tool to download and manage podcast episodes from France Radio's API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "frpd": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch src/index.tsx",
    "start": "bun dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "podcast",
    "france-radio",
    "cli",
    "downloader"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.2",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "ink": "^5.0.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.45",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
